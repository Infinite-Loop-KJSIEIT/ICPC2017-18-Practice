#include <stdio.h>

int main()
{
    int T;
    double a,b,c,x,y,min;
    scanf("%d",&T);
    while(T-->0)
    {
        scanf("%lf %lf %lf %lf %lf",&a,&b,&c,&x,&y);
        if((a+b+c)!=(x+y))
            printf("\nNO");
        else
        {
            min=(a<b)?((a<c)?a:c):((b<c)?b:c);
            if(x<min||y<min)
                printf("\nNO");
            else
                printf("\nYES");
        }
    }
    return 0;
}
