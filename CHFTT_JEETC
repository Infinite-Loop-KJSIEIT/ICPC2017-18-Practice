#include<stdio.h>
#include<string.h>
int main(void){
    int T,x,y;
    scanf("%d",&T);
    while(T-->0)
    {
        int i,l;
        x=0;
        y=0;
        char s[100];
        scanf("%s",s);
        l=strlen(s);
        for(i=0;i<l;i++)
        {
         if(s[i]=='1')
         x++;
         else if(s[i]=='0')
         y++;
         if((x==10)&&(y==10))
			{
				x=9;
				y=9;
			}
			if(x==11)
			{
				printf("WIN\n");
				break;
			}
			if(y==11)
			{
				printf("LOSE\n");
				break;
			}
        }	
    }
    return 0;
}
